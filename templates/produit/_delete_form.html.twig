<form method="post" action="{{ path('app_produit_delete', {'id': produit.id}) }}" onsubmit="return confirm('Êtes-vous sûr de bien vouloir supprimer cet élément?');">
   <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ produit.id) }}">

   <button class="btn btn-sm btn-primary">Supprimer</button>
</form>


